.header {
    background: #FFE200;
}

.header-content {}

.content {
    box-sizing: border-box;
    padding: 40rpx 30rpx 160rpx;
}

.role-box {
    margin-bottom: 42rpx;
}

.role-item {
    border: 1rpx solid #EEEEEE;
    border-radius: 14rpx;
    margin-bottom: 40rpx;
}

.role-img {
    width: 100%;
    height: 284rpx;
}

.role-btn {
    width: 324rpx;
    line-height: 128rpx;
    background: #FFE200;
    border-radius: 64rpx;
    text-align: center;
    font-size: 42rpx;
    color: #333333;
    text-align: center;
    font-weight: 700;
    margin: 50rpx auto;
}

.btn-back {
    width: 340rpx;
    line-height: 70rpx;
    border: 1rpx solid #EEEEEE;
    border-radius: 36rpx;
    margin: 0 auto;
}

.icon-back {
    width: 32rpx;
    height: 32rpx;
}

.back-txt {
    padding-left: 20rpx;
    font-size: 32rpx;
    color: #999999;
    font-weight: 700;
}
